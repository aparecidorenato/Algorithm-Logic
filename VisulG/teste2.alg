algoritmo "semnome"
var
  c, n1, n2, resul : inteiro
inicio
 n1<- 1
 n2<- 2
 resul<- 0
 c<- 0
 escreva (resul)
        resul<- +1
        escreva (resul)
        para c<- 0 ate 15 faca
             escreva (resul)
             resul<- resul + n1
             n1<- n1 + resul
        fimpara

fimalgoritmo

