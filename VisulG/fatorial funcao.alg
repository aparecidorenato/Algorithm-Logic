algoritmo "semnome"
var
   N, F: inteiro
   
funcao fatorial (v: inteiro): inteiro
var c, r : inteiro
inicio
   R<- 1
   para c<- 1 ate V faca
        R<- R*C
   fimPara
   Retorne R
fimFuncao

inicio
   Escreva ("Digite um numero: ")
   Leia (N)
   F<- Fatorial (N)
   Escreval (" O valor de ", N, "! e igual a ", F)
fimalgoritmo
